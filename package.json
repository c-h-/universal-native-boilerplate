{
  "name": "universal-native-boilerplate",
  "version": "0.0.1",
  "author": "Charlie Hulcher",
  "private": true,
  "scripts": {
    "setup": "gulp setup",
    "start": "gulp -h",
    "start:packager": "node node_modules/react-native/local-cli/cli.js start",
    "start:packager:web": "webpack-dev-server --config web/webpack.config.js --inline",
    "start:packager:macos": "node node_modules/react-native-macos/local-cli/cli.js start",
    "build:production:web": "cross-env NODE_ENV=production webpack --config web/webpack.config.js --progress",
    "build:debug:web": "webpack --config web/webpack.config.js --progress",
    "start:debug:server": "nodemon build/server/debug/main.js --ignore js/ --ignore web/ --ignore index.web.js",
    "start:production:web": "serve build/web/production -s -c 1",
    "test": "jest",
    "lint": "eslint -c .eslintrc --fix js/**",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "express": "^4.14.1",
    "nodemon": "^1.11.0",
    "react": "~15.4.0",
    "react-dom": "^15.4.2",
    "react-native-i18n": "^1.0.0",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.3",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "react-native": "0.41.2",
    "react-native-windows": "^0.41.0-rc.0",
    "react-native-web": "^0.0.70"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "fs-extra": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "jest": "18.1.0",
    "lodash.assign": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "path-exists": "^3.0.0",
    "react-test-renderer": "~15.4.0",
    "run-sequence": "^1.2.2",
    "shelljs": "^0.7.6",
    "yargs": "^6.6.0",
    "babel-preset-react-native": "^1.9.1",
    "rnpm-plugin-windows": "^0.2.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "resource-hints-webpack-plugin": "^0.0.1",
    "serve": "2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "1.14.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native-macos": {
    "react-native": "react-native-macos"
  }
}
